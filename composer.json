{
  "name": "berthott/laravel-locale-middleware",
  "description": "Laravel Helper for localizing API routes",
  "license": "MIT",
  "authors": [
      {
          "name": "Jan Bladt",
          "email": "berthott@web.de"
      }
  ],
  "autoload" : {
      "psr-4" : {
          "berthott\\LocaleMiddleware\\": "src"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "berthott\\LocaleMiddleware\\Tests\\": "tests"
      }
  },
  "extra" : {
      "laravel" : {
          "providers" : [
              "berthott\\LocaleMiddleware\\LocaleMiddlewareServiceProvider"
          ]
      }
  },
  "scripts": {
        "test": "vendor/bin/phpunit"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0",
    "phpunit/phpunit": "^10.0"
  }
}
